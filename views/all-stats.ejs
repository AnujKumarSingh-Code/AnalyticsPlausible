<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
<iframe plausible-embed src="https://plausible.io/share/analyticsplausible-1.onrender.com?auth=2HAEsFBoAdHFpdG5e31W9&embed=true&theme=light" scrolling="no" frameborder="0" loading="lazy" style="width: 1px; min-width: 100%; height: 1600px;"></iframe>
<div style="font-size: 14px; padding-bottom: 14px;">Stats powered by <a target="_blank" style="color: #4F46E5; text-decoration: underline;" href="https://plausible.io">Plausible Analytics</a></div>
<script async src="https://plausible.io/js/embed.host.js"></script>
<script defer data-domain="analyticsplausible-1.onrender.com" src="https://plausible.io/js/script.outbound-links.pageview-props.tagged-events.js"></script>
    <script defer data-domain="7535-2409-4081-2e1b-811d-7c22-6e24-f5d9-e905.ngrok-free.app" src="https://plausible.io/js/script.outbound-links.js"></script>
    <title>All Links Stats</title>
</head>
<body>
    <h1>All Links Stats</h1>
    <table>
        <thead>
            <tr>
                <th>User</th>
                <th>Link</th>
                <th>Visits</th>
            </tr>
        </thead>
        <tbody>
            <% stats.forEach(stat => { %>
                <tr>
                    <td><%= stat.username %></td>
                    <td><a href="<%= stat.url %>" target="_blank"><%= stat.url %></a></td>
                    <td><%= stat.visits %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
